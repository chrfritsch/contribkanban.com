{
    "name": "mglaman/contribkanban.com",
    "description": "Contribkanban.com",
    "type": "project",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": ">=7.3",
        "composer/installers": "^1.2",
        "consolidation/robo": "^1.1",
        "cweagans/composer-patches": "^1.6",
        "drupal/admin_toolbar": "^1.20",
        "drupal/console": "^1.0.2",
        "drupal/core-composer-scaffold": "^8.9",
        "drupal/core-recommended": "^8.9",
        "drupal/email_registration": "^1.0@RC",
        "drupal/entity": "~1.0",
        "drupal/inline_entity_form": "^1.0@beta",
        "drupal/jsonapi_hypermedia": "^1.6",
        "drupal/jsonapi_search_api": "^1.0@beta",
        "drupal/jsonapi_user_resources": "^1.0@alpha",
        "drupal/lagoon_logs": "^1.0",
        "drupal/redis": "^1.1",
        "drupal/search_api": "^1.15",
        "drupal/simple_oauth": "^4.4",
        "drupal/swiftmailer": "^1.0@beta",
        "drush/drush": "^9.0.0",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^0.6.2",
        "drupal/drupal-extension": "^3.4",
        "drupal/core-dev-pinned": "^8.9",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "bin-dir": "bin",
        "discard-changes": true,
        "optimize-autoloader": true,
        "apcu-autoloader": true,
        "sort-packages": true,
        "platform": {
            "php": "7.3"
        }
    },
    "scripts": {
        "setup": "composer install && ./bin/robo setup"
    },
    "extra": {
        "enable-patching": true,
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "drupal-scaffold": {
            "excludes": [
                "sites/default/default.services.yml",
                "sites/default/default.settings.php"
            ],
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
            "web/modules/custom/{$name}": ["type:drupal-custom-module"],
            "web/themes/custom/{$name}": ["type:drupal-custom-theme"]
        }
    }
}
